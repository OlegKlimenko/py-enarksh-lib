<?xml version="1.0" encoding="utf-8"?>
<Schedule>
 <NodeName>TEST02</NodeName>
 <UserName>test</UserName>
 <Nodes>
  <ManualTrigger>
   <NodeName>start</NodeName>
   <OutputPorts>
    <Port>
     <PortName>all</PortName>
    </Port>
   </OutputPorts>
  </ManualTrigger>
  <CompoundJob>
   <NodeName>spam_and_eggs</NodeName>
   <InputPorts>
    <Port>
     <PortName>all</PortName>
     <Dependencies>
      <Dependency>
       <NodeName>start</NodeName>
       <PortName>all</PortName>
      </Dependency>
     </Dependencies>
    </Port>
   </InputPorts>
   <Nodes>
    <CommandJob>
     <NodeName>spam</NodeName>
     <InputPorts>
      <Port>
       <PortName>all</PortName>
       <Dependencies>
        <Dependency>
         <NodeName>.</NodeName>
         <PortName>all</PortName>
        </Dependency>
       </Dependencies>
      </Port>
     </InputPorts>
     <OutputPorts>
      <Port>
       <PortName>all</PortName>
      </Port>
     </OutputPorts>
     <Path>/bin/ls</Path>
     <Args>
      <Arg>spam</Arg>
     </Args>
    </CommandJob>
    <CommandJob>
     <NodeName>eggs</NodeName>
     <InputPorts>
      <Port>
       <PortName>all</PortName>
       <Dependencies>
        <Dependency>
         <NodeName>.</NodeName>
         <PortName>all</PortName>
        </Dependency>
       </Dependencies>
      </Port>
     </InputPorts>
     <OutputPorts>
      <Port>
       <PortName>all</PortName>
      </Port>
     </OutputPorts>
     <Path>/bin/ls</Path>
     <Args>
      <Arg>eggs</Arg>
     </Args>
    </CommandJob>
   </Nodes>
   <OutputPorts>
    <Port>
     <PortName>all</PortName>
     <Dependencies>
      <Dependency>
       <NodeName>spam</NodeName>
       <PortName>all</PortName>
      </Dependency>
      <Dependency>
       <NodeName>eggs</NodeName>
       <PortName>all</PortName>
      </Dependency>
     </Dependencies>
    </Port>
   </OutputPorts>
  </CompoundJob>
  <Terminator>
   <NodeName>end</NodeName>
   <InputPorts>
    <Port>
     <PortName>all</PortName>
     <Dependencies>
      <Dependency>
       <NodeName>spam_and_eggs</NodeName>
       <PortName>all</PortName>
      </Dependency>
     </Dependencies>
    </Port>
   </InputPorts>
  </Terminator>
 </Nodes>
</Schedule>
